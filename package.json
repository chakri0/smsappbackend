{
	"name": "pr-developer",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"ts-node": "ts-node",
		"format": "prettier --ignore-unknown --write .",
		"lint": "npx eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "npm run lint --fix",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"migration-gen": "npm run typeorm migration:generate -- -d src/database/dataSource.ts",
		"migration-run": "npx typeorm-ts-node-esm migration:run -d src/database/dataSource.ts",
		"start": "nodemon src/server.ts",
		"prepare": "husky install",
		"build": "tsc",
		"prod": "npm run build && node build/server.js"
	},
	"author": "KavalakuntlaChakradhar",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^18.15.11",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.0",
		"memcached": "^2.2.2",
		"mysql2": "^3.2.0",
		"nodemailer": "^6.9.1",
		"nodemon": "^2.0.22",
		"openai": "^3.2.1",
		"otp-generator": "^4.0.1",
		"path": "^0.12.7",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.9.1",
		"typeorm": "^0.3.14",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-conventional": "^18.1.0",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.16",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/nodemailer": "^6.4.13",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"eslint": "^8.53.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.0.2",
		"prettier": "3.0.3",
		"typescript": "^5.2.2"
	}
}